                        </LineChart>
                        <LineChart fx:id="bothMean" layoutX="422.0" layoutY="395.0" prefHeight="350.0" prefWidth="350.0" title="Mean">
                           <xAxis>
                              <NumberAxis fx:id="xAxis" side="BOTTOM" />
                           </xAxis>
                           <yAxis>
                              <NumberAxis fx:id="yAxis" side="LEFT" />
                           </yAxis>
                        </LineChart>
                        <Label layoutX="387.0" layoutY="5.0" text="Both" />
                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab text="Weasle">
          <content>
          		  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
          		  </AnchorPane>
          	</content>

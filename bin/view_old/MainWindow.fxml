            <Tab text="Both">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <LineChart fx:id="bothZScore"  layoutX="34.0" layoutY="14.0" prefHeight="350.0" prefWidth="350.0" title="Z-Scores">
                          <xAxis>
                            <NumberAxis fx:id="xAxis" side="BOTTOM" />
                          </xAxis>
                          <yAxis>
                            <NumberAxis fx:id="yAxis" side="LEFT" />
                          </yAxis>
                        </LineChart>
                        <LineChart fx:id="bothVOT" layoutX="422.0" layoutY="14.0" prefHeight="350.0" prefWidth="350.0" title="Values over Time">

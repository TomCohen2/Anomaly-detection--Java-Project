<BorderPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MainWindowController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
      <TabPane prefHeight="800.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
          <Tab text="TrainFlight">
            <content>
              <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="0.0" minWidth="0.0" prefHeight="1200.0" prefWidth="1200.0">
                     <children>
                        <LineChart fx:id="trainVOT" layoutX="422.0" layoutY="14.0" prefHeight="350.0" prefWidth="350.0" title="Values over Time">
                          <xAxis>
                            <NumberAxis side="BOTTOM" fx:id="xAxis" />
                          </xAxis>
                          <yAxis>
                            <NumberAxis fx:id="yAxis" side="LEFT" />
                          </yAxis>
                        </LineChart>
                        <LineChart fx:id="trainZScoreTx" layoutX="34.0" layoutY="14.0" prefHeight="350.0" prefWidth="350.0" title="Z-Score &amp; Tx">
                          <xAxis>
                            <NumberAxis side="BOTTOM" fx:id="xAxis" />
                          </xAxis>
                          <yAxis>
                            <NumberAxis fx:id="yAxis" side="LEFT" />
                          </yAxis>
                        </LineChart>
                        <LineChart fx:id="trainSTDDev" layoutX="34.0" layoutY="395.0" prefHeight="350.0" prefWidth="350.0" title="Standard Deviation">
                          <xAxis>
                            <NumberAxis side="BOTTOM" fx:id="xAxis" />
                          </xAxis>
                          <yAxis>
                            <NumberAxis fx:id="yAxis" side="LEFT" />
                          </yAxis>
                        </LineChart>
                        <LineChart fx:id="trainMean" layoutX="422.0" layoutY="395.0" prefHeight="350.0" prefWidth="350.0" title="Mean">
                          <xAxis>
                            <NumberAxis side="BOTTOM" fx:id="xAxis" />
                          </xAxis>
                          <yAxis>
                            <NumberAxis fx:id="yAxis" side="LEFT" />
                          </yAxis>
                        </LineChart>
                        <Label layoutX="370.0" layoutY="5.0" text="Train Flight" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="TestFlight">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <LineChart fx:id="testZScore" layoutX="34.0" layoutY="14.0" prefHeight="350.0" prefWidth="350.0" title="Z-Scores">
                          <xAxis>
                            <NumberAxis fx:id="xAxis" side="BOTTOM" />
